{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "plugin:import/recommended",
        "plugin:jsx-a11y/recommended",
        "plugin:react-hooks/recommended"
    ],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [ "react" ],
    "rules": {
        "indent": [ "error", "tab" ],
        "linebreak-style": [ "error", "windows" ],
        "quotes": [ "error", "double" ],
        "semi": [ "error", "always" ],
        "react/react-in-jsx-scope": "off"
    }
}
